{
  "name": "vue-ts-types",
  "version": "1.6.0",
  "description": "Lightweight TypeScript-first Vue prop type definitions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "type-test": "jest -c jest.config.tsd.ts",
    "type-test:watch": "jest -c jest.config.tsd.ts --watch",
    "lint": "npm run lint:eslint && npm run lint:markdownlint",
    "lint:eslint": "eslint .",
    "lint:markdownlint": "markdownlint \"**/*.md\"",
    "build": "tsc --project tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FloEdelmann/vue-ts-types.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "vuejs",
    "vuejs2",
    "vuejs3",
    "typescript",
    "prop-types",
    "props",
    "proptype-validators",
    "proptypes",
    "hacktoberfest"
  ],
  "author": "Flo Edelmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FloEdelmann/vue-ts-types/issues"
  },
  "homepage": "https://github.com/FloEdelmann/vue-ts-types",
  "devDependencies": {
    "@tsd/typescript": "^5.0.4",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-runner-tsd": "^5.0.0",
    "markdownlint-cli": "^0.34.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vue": "^3.3.4",
    "vue2-6": "npm:vue@~2.6.14",
    "vue2-7": "npm:vue@~2.7.5"
  },
  "peerDependencies": {
    "vue": "^2.6 || ^3.2"
  }
}
